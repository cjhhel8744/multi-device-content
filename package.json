{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "nuxt dev --dotenv .env.development --open",
		"dev:local": "nuxt dev --dotenv .env.local --open",
		"build": "npm run clean && npm i && nuxt build --dotenv .env.production && npm run deploy:nuxt",
		"build:local": "npm run clean && npm i && nuxt build --dotenv .env.local && npm run deploy:nuxt",
		"build:dev": "npm run clean && npm i && nuxt build --dotenv .env.development && npm run deploy:nuxt",
		"build:stage": "npm run clean && npm i && nuxt build --dotenv .env.stage && npm run deploy:nuxt",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"nitro:build": "nitro build",
		"deploy:nuxt": "copyfiles -u 1 ./.output/**/*.* ../deploy/.output && copyfiles -a ./web.config ../deploy/.output/ && copyfiles -a ./.env.* ../deploy/.output/ && npm run excludeclean",
		"start": "node .output/server/index.mjs",
		"start-build": "nuxi build && node .output/server/index.mjs",
		"lint": "eslint \"{**/*,*}.{js,ts,tsx,vue}\"",
		"lint:fix": "npm run lint --fix",
		"clean": "node ./tasks/clean.cjs",
		"excludeclean": "node ./tasks/excludeclean.cjs",
		"copyfiletest": "node -e \"if(require('fs').existsSync('../deploy/.output/.output/public/HTML_TEST')) console.log(require('fs').rmdirSync('../deploy/.output/.output/public/HTML_TEST'))\""
	},
	"devDependencies": {
		"@nuxt/devtools": "latest",
		"@nuxt/kit": "^3.8.2",
		"@nuxt/schema": "^3.8.2",
		"@nuxtjs/device": "^3.1.1",
		"@nuxtjs/eslint-config-typescript": "^12.1.0",
		"@pinia/nuxt": "^0.5.1",
		"@types/node": "^20.9.4",
		"@types/serialize-javascript": "^5.0.4",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"@vite-pwa/nuxt": "^0.2.3",
		"@vitejs/plugin-vue": "^4.5.0",
		"@vue-macros/nuxt": "^1.9.6",
		"@vue/compiler-sfc": "^3.3.8",
		"@vueuse/components": "^10.6.1",
		"@vueuse/core": "^10.6.1",
		"@vueuse/nuxt": "^10.6.1",
		"consola": "^3.2.3",
		"copyfiles": "^2.4.1",
		"core-js": "^3.33.3",
		"dotenv": "^16.3.1",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-vitest": "^0.3.10",
		"eslint-plugin-vue": "^9.18.1",
		"nuxt": "^3.8.2",
		"nuxt-typed-router": "^3.3.3",
		"prettier": "^3.1.0",
		"typescript": "^5.3.2",
		"vitest": "^0.34.6",
		"vue": "^3.3.8",
		"vue-router": "^4.2.5",
		"vue-tsc": "^1.8.22"
	},
	"dependencies": {
		"h3-proxy": "^1.10.0",
		"pinia": "^2.1.7"
	}
}
